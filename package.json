  "repository": {
    "type": "git",
    "url": "http://github.com/superwf/mobx-value"
  },
  "source": "src/index.ts",
  "files": [
    "module",
    "dist",
    "lib",
    "src",
    "type"
  ],
  "keywords": [
    "mobx",
    "mobservable",
    "observable",
    "data flow",
    "state management",
    "data management",
    "redux",
    "request",
    "model",
    "react",
    "reactjs"
