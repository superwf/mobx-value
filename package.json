  "scripts": {
    "test-default": "jest",
    "test-mobx60": "MOBX60=true jest",
    "test": "yarn test-default && yarn test-mobx60",
    "start": "NODE_ENV=development webpack serve -c webpack.config.demo.ts",
    "clean": "rimraf {module,dist,lib,type} -f",
    "badge": "jest-coverage-badges output ./badge",
    "build": "tsc -p tsconfig.lib.json && tsc -p tsconfig.module.json && tsc -p tsconfig.declaration.json && NODE_ENV=production webpack && NODE_ENV=production webpack -c webpack.config.standalone.ts && cp type/* module/ && cp type/* dist/ && cp type/* lib/",
    "prepublishOnly": "yarn test && yarn build && standard-version",
    "prepare": "husky install",
    "publish-rc": "yarn test && yarn build && yarn standard-version -- prerelease rc && npm publish --ignore-scripts"
  },
  "sideeffects": false,
  "main": "lib/index.js",
  "types": "type/index.d.ts",
  "typings": "type/index.d.ts",
  "module": "module/index.js",
  "unpkg": "dist/index.js",
  "umd:main": "dist/index.js",
  "jsdelivr": "dist/index.js",
